<html>
<head>
	<title>Post lists</title>
</head>
<body>
	<% @posts.each do |post| %>

	<%= link_to edit_post_path(post) do %>
	<div>
		<%= post.title %>
		<br>
		<%= post.author %>
		<br>
		<%= post.description %>
		<br>
		$<%= post.price %>
	</div>
	<% end %>
	<%= link_to "copy", copy_post_path(post.id), method: :post %>
	<%= link_to "delete", delete_post_path(post.id), method: :delete %>
	<br>
	<% end %>

</body>
</html>